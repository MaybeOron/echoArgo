apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: echo-ingress
  annotations:
    kubernetes.io/ingress.class: nginx
    cert-manager.io/cluster-issuer: letsencrypt
  labels:
    name: echo-ingress
spec:
  tls:
  - hosts:
    - oronargo.tk
    secretName: echo-secret-tls
    #secretName: echo-secret-tls-sec
    #secretName: echo-secret-tls-q8ths
  rules:
  - host: oronargo.tk
    http:
      paths:
      - pathType: Prefix
        path: "/"
        backend:
          service:
            name: echo-yaml-ip
            port: 
              number: 80
